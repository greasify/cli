{
  "name": "@greasify/cli",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "gy": "./dist/bin.js",
    "greasify": "./dist/bin.js"
  },
  "scripts": {
    "start": "pnpm build && node dist/bin.js",
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,ts,tsx,json}'"
  },
  "devDependencies": {
    "@crashmax/prettier-config": "3.2.1",
    "@crashmax/tsconfig": "2.0.1",
    "@types/node": "18.15.11",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@types/yargs": "17.0.24",
    "yargs": "17.7.1"
  }
}
