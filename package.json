{
  "name": "@greasify/cli",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "gy": "./dist/bin.js",
    "greasify": "./dist/bin.js"
  },
  "scripts": {
    "start": "pnpm build && node dist/bin.js",
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,ts,tsx,json}'",
    "run-init": "pnpm build && node dist/bin.js init"
  },
  "devDependencies": {
    "@crashmax/prettier-config": "5.0.1",
    "@crashmax/tsconfig": "2.1.0",
    "@octokit/types": "12.4.0",
    "@types/adm-zip": "0.5.5",
    "@types/node": "20.10.4",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@inquirer/prompts": "3.3.0",
    "@types/yargs": "17.0.32",
    "adm-zip": "0.5.10",
    "decamelize": "6.0.0",
    "yargs": "17.7.2"
  }
}
